{
  "name": "ionic-hello-world",
  "author": "Ionic Framework",
  "homepage": "http://ionicframework.com/",
  "private": true,
  "scripts": {
    "build": "ionic-app-scripts build",
    "build_dev": "ionic-app-scripts build --dev",
    "watch": "ionic-app-scripts watch",
    "serve:before": "watch",
    "emulate:before": "build",
    "deploy:before": "build",
    "build:before": "build",
    "run:before": "build",
    "device": "ionic-app-scripts build --dev && cordova run android --device"
  },
  "dependencies": {
    "ionic-angular": "^2.0.0-rc.0",
    "ionicons": "^3.0.0",
    "@ionic/storage": "^1.0.3",
    "ionic-native": "^2.0.3"
  },
  "devDependencies": {
    "@ionic/app-scripts": "latest",
    "typescript": "^2.0.3"
  },
  "cordovaPlugins": [
    "cordova-plugin-device",
    "cordova-plugin-console",
    "cordova-plugin-whitelist",
    "cordova-plugin-splashscreen",
    "cordova-plugin-statusbar",
    "ionic-plugin-keyboard",
    {
      "id": "cordova-plugin-googlemaps",
      "locator": "https://github.com/mapsplugin/cordova-plugin-googlemaps.git#multiple_maps",
      "variables": {
        "API_KEY_FOR_ANDROID": "API_KEY_FOR_ANDROID",
        "API_KEY_FOR_IOS": "API_KEY_FOR_IOS"
      }
    }
  ],
  "cordovaPlatforms": [
    "ios",
    "android"
  ],
  "description": "cutePuppyPics: An Ionic project"
}
